<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.present.productionValuesController">
   <children>
      <Button fx:id="Confirm" layoutX="157.0" layoutY="361.0" mnemonicParsing="false" onAction="#confirmProduction" text="Confirm">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Text layoutX="92.0" layoutY="44.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Opret Produktion">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Text>
      <TextField fx:id="IDTextField" layoutX="92.0" layoutY="210.0" prefHeight="25.0" prefWidth="192.0" promptText="Skriv produktionens ID" />
      <TextField fx:id="nameTextField" layoutX="92.0" layoutY="243.0" prefHeight="25.0" prefWidth="192.0" promptText="Skriv produktionens titel" />
      <TextField fx:id="genreTextField" layoutX="92.0" layoutY="282.0" prefHeight="25.0" prefWidth="192.0" promptText="Skriv produktionens genre" />
      <TextField fx:id="releaseYearTextField" layoutX="92.0" layoutY="324.0" prefHeight="25.0" prefWidth="192.0" promptText="Skriv produktionens udgivelsesår" />

      <Button fx:id="backButton" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#switchToProduction" prefWidth="32.0" styleClass="BackIcon" stylesheets="@../../CSS/BackArrow.css" />
      <ImageView fitHeight="128.0" fitWidth="125.0" layoutX="24.0" layoutY="63.0" pickOnBounds="true" />
      <TextArea layoutX="206.0" layoutY="63.0" prefHeight="128.0" prefWidth="348.0" promptText="Beskrivelse af Produktion">
         <font>
            <Font size="20.0" />
         </font>
      </TextArea>
      <Text fx:id="idProductionColumn" layoutX="21.0" layoutY="227.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ID" />
      <Text fx:id="nameProductionColumn" layoutX="21.0" layoutY="260.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Titel" />
      <Text fx:id="genreProductionColumn" layoutX="17.0" layoutY="299.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Genre" />
      <Text fx:id="releaseProductionYearColumn" layoutX="15.0" layoutY="341.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Udgivelsesår" />
   </children>
</AnchorPane>
