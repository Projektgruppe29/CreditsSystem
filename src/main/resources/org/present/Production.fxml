<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.present.ProductionController">
    <children>
        <Button fx:id="createProductionButton" layoutX="48.0" layoutY="76.0" mnemonicParsing="false" onAction="#switchToProductionValues" text="Create production" />
        <Button fx:id="viewProductionButton" layoutX="50.0" layoutY="132.0" mnemonicParsing="false" onAction="#showProduction" text="View productions" />
        <Button fx:id="backButton" layoutX="48.0" layoutY="21.0" mnemonicParsing="false" onAction="#backToMainScene" prefHeight="25.0" prefWidth="23.0" styleClass="BackIcon" stylesheets="@../../CSS/BackArrow.css" />
        <Button fx:id="creditButton" layoutX="52.0" layoutY="188.0" mnemonicParsing="false" onAction="#SwitchToCredits" prefHeight="25.0" prefWidth="109.0" text="Create credits" />
      <TableView fx:id="tableView" layoutX="253.0" layoutY="76.0" prefHeight="200.0" prefWidth="312.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="genreColumn" prefWidth="75.0" text="Genre" />
            <TableColumn fx:id="releaseYearColumn" prefWidth="75.0" text="Release Year" />
        </columns>
      </TableView>
    </children>
</AnchorPane>
